{
  "manifest_version": 2,

  "name": "Question Generator",
  "description": "Generates Questions from given Sentences",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "mypopup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "storage"
  ],
  "background":{
    "scripts" : ["event.js"],
    "persistent": false

  }
}
